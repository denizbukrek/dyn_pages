{
  "name": "client-api",
  "version": "0.0.1",
  "main": "server.js",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect=9231 dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "@sendgrid/mail": "7.4.2",
    "@turf/turf": "5.1.6",
    "@types/multer": "^1.4.5",
    "aws-sdk": "^2.880.0",
    "bluebird": "3.7.2",
    "body-parser": "1.19.0",
    "class-transformer": "0.1.10",
    "dotenv": "5.0.1",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "i18n": "^0.13.2",
    "jwt-simple": "0.5.6",
    "md5": "2.3.0",
    "method-override": "2.3.10",
    "moment": "2.29.1",
    "morgan": "1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "osrm-text-instructions": "0.13.4",
    "otplib": "9.0.0",
    "passport": "0.4.1",
    "passport-facebook-token": "3.3.0",
    "passport-http": "0.3.0",
    "passport-http-bearer": "1.0.1",
    "pg-monitor": "1.4.1",
    "pg-promise": "10.9.5",
    "request": "2.88.2",
    "route-parser": "0.0.5",
    "routes": "2.1.0",
    "sendwithus": "4.3.1",
    "sharp": "^0.28.3",
    "stack-trace": "0.0.10",
    "twilio": "3.59.0",
    "validate-image-type": "^1.1.1"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.20",
    "@types/dotenv": "4.0.3",
    "@types/express": "4.11.1",
    "@types/express-session": "1.15.8",
    "@types/jwt-simple": "0.5.33",
    "@types/md5": "2.1.32",
    "@types/method-override": "0.0.31",
    "@types/morgan": "1.7.35",
    "@types/passport": "0.4.5",
    "@types/passport-facebook-token": "0.4.33",
    "@types/passport-http": "0.3.6",
    "@types/passport-http-bearer": "1.0.33",
    "@types/request": "2.47.0",
    "@types/route-parser": "0.1.1",
    "concurrently": "3.5.1",
    "nodemon": "1.19.4",
    "ts-node": "6.0.3",
    "typescript": "4.2.2"
  }
}
